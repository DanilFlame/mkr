.data
    result: .word 1      # Змінна для збереження результату
    n:      .word 6      # Змінна для збереження значення n

.text
.globl main
main:
    # Завантаження значення n в регістр $t0
    lw $t0, n

    # Перевірка, чи n = 0 або n = 1
    beqz $t0, fact_end
    li $t1, 1
    beq $t0, $t1, fact_end

    # Ініціалізація результату як 1
    li $t2, 1

fact_loop:
    mul $t2, $t2, $t0    # Помножити результат на поточне значення $t0
    addi $t0, $t0, -1    # Зменшити значення $t0 на 1
    bgtz $t0, fact_loop  # Якщо $t0 > 0, повторити цикл

fact_end:
    # Збереження результату у регістр $t6
    move $t6, $t2

    # Завершення програми
    li $v0, 10
    syscall
